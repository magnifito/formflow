{
   "name": "formflow-dashboard-api",
   "version": "0.0.1",
   "description": "FormFlow Dashboard API - Dashboard and admin backend with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@jest/types": "^30.2.0",
      "@types/bcrypt": "^6.0.0",
      "@types/cors": "^2.8.19",
      "@types/jest": "^30.0.0",
      "@types/jsonwebtoken": "^9.0.10",
      "@types/node": "^25.0.10",
      "@types/nodemailer": "^7.0.5",
      "@types/supertest": "^6.0.3",
      "jest": "^30.2.0",
      "supertest": "^7.2.2",
      "ts-jest": "^29.4.6",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.9.3"
   },
   "dependencies": {
      "axios": "^1.13.2",
      "bcrypt": "^6.0.0",
      "bcryptjs": "^3.0.3",
      "body-parser": "^2.2.2",
      "cors": "^2.8.6",
      "dotenv": "^17.2.3",
      "express": "^5.2.1",
      "jsonwebtoken": "^9.0.3",
      "nodemailer": "^7.0.12",
      "pg": "^8.17.2",
      "reflect-metadata": "^0.2.2",
      "typeorm": "0.3.28",
      "uuid": "^13.0.0",
      "winston": "^3.19.0",
      "winston-daily-rotate-file": "^5.0.0"
   },
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
      "start": "ts-node src/index.ts",
      "typeorm": "ts-node ../../node_modules/typeorm/cli.js",
      "migration:generate": "ts-node --project tsconfig.json -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:generate -d src/data-source.ts",
      "migration:run": "ts-node --project tsconfig.json -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
      "migration:revert": "ts-node --project tsconfig.json -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
      "migration:show": "ts-node --project tsconfig.json -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:show -d src/data-source.ts",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "test:verbose": "jest --verbose"
   }
}