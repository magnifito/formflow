{
  "name": "formflow-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "FormFlow monorepo - form data handling made easy",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "nx": "nx",
    "// --- Setup ---": "",
    "setup": "pnpm install && pnpm sync-deps && pnpm setup:env && echo '✓ Setup complete! Run: pnpm dev'",
    "setup:env": "bash scripts/setup-dev-env.sh",
    "setup:env:copy": "if [ ! -f .env.development ] && [ ! -f .env ]; then cp .env.development.example .env.development; fi && echo '✓ Environment file ready'",
    "sync-deps": "pnpm exec ts-node scripts/sync-deps.ts",
    "postinstall": "nx reset",
    "// --- Development ---": "",
    "dev": "docker-compose -f docker-compose.db.yml up -d --wait && NX_TUI=false nx run-many --target=dev --projects=dashboard-api,collector-api,dashboard-ui,test-lab --parallel=4",
    "dashboard-ui": "nx run dashboard-ui:serve",
    "dashboard-api": "nx run dashboard-api:dev",
    "collector-api": "nx run collector-api:dev",
    "test-lab": "node apps/test-lab/server.js",
    "test-lab:webhooks": "node apps/test-lab/server-webhooks.js",
    "// --- Database Seeding ---": "",
    "seed": "pnpm exec ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/seed-sample-data.ts",
    "seed:mailpit": "pnpm exec ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/seed-mailpit-acme.ts",
    "// --- Environment ---": "",
    "test:env": "pnpm exec ts-node-esm scripts/test-env-config.ts",
    "// --- Build ---": "",
    "build": "nx run-many --target=build --all",
    "build:dashboard-ui": "nx build dashboard-ui",
    "build:dashboard-api": "nx build dashboard-api",
    "build:collector-api": "nx build collector-api",
    "// --- Testing ---": "",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --codeCoverage",
    "test:coverage:ci": "nx run-many --target=test --all --configuration=ci",
    "test:dashboard-ui": "nx test dashboard-ui",
    "test:dashboard-ui:watch": "nx test dashboard-ui --watch",
    "test:dashboard-ui:coverage": "nx test dashboard-ui --codeCoverage",
    "test:dashboard-ui:coverage:ci": "nx test dashboard-ui --configuration=ci",
    "test:dashboard-api": "nx test dashboard-api",
    "test:dashboard-api:watch": "nx test dashboard-api --watch",
    "test:dashboard-api:coverage": "nx test dashboard-api --codeCoverage",
    "test:dashboard-api:coverage:ci": "nx test dashboard-api --configuration=ci",
    "test:collector-api": "nx test collector-api",
    "test:collector-api:watch": "nx test collector-api --watch",
    "test:collector-api:coverage": "nx test collector-api --codeCoverage",
    "test:collector-api:coverage:ci": "nx test collector-api --configuration=ci",
    "test:apis": "pnpm test:dashboard-api && pnpm test:collector-api",
    "test:e2e": "pnpm test:apis",
    "test:scripts": "jest --config scripts/jest.config.js scripts/package-scripts.test.ts",
    "lint": "nx run-many --target=lint --all",
    "// --- Docker (database only for dev) ---": "",
    "db:up": "docker-compose -f docker-compose.db.yml up -d",
    "db:down": "docker-compose -f docker-compose.db.yml down",
    "db:reset": "docker-compose -f docker-compose.db.yml down -v && docker-compose -f docker-compose.db.yml up -d --wait && pnpm db:migrate",
    "db:destroy": "docker-compose -f docker-compose.db.yml down -v && echo '✓ Database completely wiped. Run pnpm db:up to start fresh.'",
    "// --- Docker Full Stack (alternative) ---": "",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "// --- Docker Production ---": "",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "// --- Docker Build ---": "",
    "docker:build": "nx run dashboard-api:docker:build && nx run collector-api:docker:build",
    "docker:build:dashboard-api": "nx run dashboard-api:docker:build",
    "docker:build:collector-api": "nx run collector-api:docker:build",
    "// --- Nx Utilities ---": "",
    "graph": "nx graph",
    "affected": "nx affected:graph",
    "affected:test": "nx affected -t test",
    "affected:build": "nx affected -t build",
    "reset": "nx reset",
    "nx:reset": "nx reset",
    "// --- Migrations ---": "",
    "db:migrate": "pnpm --filter dashboard-api migration:run",
    "db:migrate:revert": "pnpm --filter dashboard-api migration:revert",
    "db:migrate:show": "pnpm --filter dashboard-api migration:show",
    "db:migrate:generate": "pnpm --filter dashboard-api migration:generate",
    "migrate:create-super-admin": "cd apps/dashboard-api && pnpm exec ts-node -r tsconfig-paths/register src/migrations/manage-super-admin.ts",
    "// --- Cleanup ---": "",
    "clean": "rm -rf dist/ node_modules/.cache",
    "clean:all": "rm -rf dist/ node_modules/ .nx/ && find apps -name node_modules -type d -exec rm -rf {} + 2>/dev/null || true",
    "clean:docker": "docker-compose -f docker-compose.db.yml -f docker-compose.yml -f docker-compose.dev.yml down -v --rmi all && docker network prune -f && echo '✅ Docker cleanup complete!'",
    "nuke": "docker-compose -f docker-compose.db.yml -f docker-compose.yml -f docker-compose.dev.yml down -v --rmi all 2>/dev/null; docker network prune -f 2>/dev/null; rm -rf dist/ node_modules/ .nx/ package-lock.json pnpm-lock.yaml .env .env.development logs/ coverage/ && find apps -name node_modules -type d -exec rm -rf {} + 2>/dev/null; echo '✅ Project nuked! Run: pnpm run setup'",
    "cycle": "pnpm run nuke && pnpm run db:destroy && pnpm run setup && pnpm run dev",
    "docker:prune:networks": "docker network prune -f && echo '✅ Orphaned networks removed'"
  },
  "devDependencies": {
    "@nx/cypress": "22.4.1",
    "@nx/eslint": "^22.4.1",
    "@nx/eslint-plugin": "^22.4.1",
    "@nx/jest": "^22.4.1",
    "@nx/js": "^22.4.1",
    "@nx/node": "^22.4.1",
    "@nx/react": "^22.4.1",
    "@nx/vite": "22.4.1",
    "@nx/vitest": "22.4.1",
    "@nx/web": "22.4.1",
    "@nx/workspace": "^22.4.1",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.10",
    "@swc/helpers": "~0.5.18",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "16.3.2",
    "@types/bs58": "^5.0.0",
    "@types/node": "25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "cypress": "^15.9.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "jiti": "2.6.1",
    "jsdom": "~27.4.0",
    "nx": "^22.4.1",
    "postcss": "^8.5.6",
    "prettier": "~3.8.1",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typeorm": "0.3.28",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@types/nodemailer": "^7.0.5",
    "axios": "^1.13.2",
    "bs58": "^6.0.0",
    "globals": "^17.1.0",
    "nodemailer": "^7.0.12",
    "pg-boss": "^12.5.4",
    "three": "^0.170.0",
    "typescript-eslint": "^8.53.1",
    "uuid": "^13.0.0"
  },
  "pnpm": {
    "overrides": {
      "glob": "^11.0.1",
      "inflight": "npm:lru-cache@^11.0.2",
      "keygrip": "^1.1.0",
      "whatwg-encoding": "^3.1.1",
      "html-encoding-sniffer": "^6.0.0"
    }
  }
}
