{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "ES2022"
    ],
    "declaration": true,
    "declarationMap": true,
    "outDir": "../../../dist/out-tsc/libs/shared/env",
    "tsBuildInfoFile": "../../../dist/out-tsc/libs/shared/env/tsconfig.tsbuildinfo",
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "composite": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "jest.config.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts"
  ]
}